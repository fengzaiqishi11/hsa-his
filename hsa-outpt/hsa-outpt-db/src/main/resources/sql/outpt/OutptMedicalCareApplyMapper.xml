    <insert id="insertMedicDate">
        insert into outpt_medical_care_apply(id,hosp_code, profile_id, visit_id, name, gender_code, age, age_unit_code, cert_no, phone, change_type, visit_type, apply_dept_id, apply_company_code, apply_company_name, hope_in_time, apply_id, apply_name, apply_time, is_house, house_bed, nusre_type_code, reality_in_time, status_code, visit_dept_id, visit_doctor_id, visit_doctor_name, visit_time, remark, crte_id, crte_name, crte_time)
        values (#{id},#{hospCode}, #{profileId}, #{visitId}, #{name}, #{genderCode}, #{age}, #{ageUnitCode}, #{certNo}, #{phone}, #{changeType}, #{visitType}, #{applyDeptId}, #{applyCompanyCode}, #{applyCompanyName}, #{hopeInTime}, #{applyId}, #{applyName}, #{applyTime}, #{isHouse}, #{houseBed}, #{nusreTypeCode}, #{realityInTime}, #{statusCode}, #{visitDeptId}, #{visitDoctorId}, #{visitDoctorName}, #{visitTime}, #{remark}, #{crteId}, #{crteName}, #{crteTime})

    </insert>
    <select id="queryMZPage" resultType="cn.hsa.module.outpt.medictocare.dto.MedicToCareDTO">
        SELECT  id as visitId,name,cert_no as certNo ,dept_id as deptId,dept_name as deptName,"1" as visitType
        from outpt_visit
        where 
            hosp_code = #{hospCode}
        <if test="certNo != null and certNo != ''">
            and cert_no = #{certNo}
        </if>
        <if test="name != null and name != ''">
            and name = #{name}
        </if>
        <if test="visitStartTime != null and visitStartTime != ''">
            and DATE_FORMAT(crte_time,'%Y-%m-%d') &gt;=#{visitStartTime}
        </if>
        <if test="visitEndTime != null and visitEndTime != ''">
            and DATE_FORMAT(crte_time,'%Y-%m-%d') &lt;=#{visitEndTime}
        </if>
        <if test="keyword != null and keyword != ''">
            and (name like concat('%',#{keyword},'%') or cert_no like concat('%',#{keyword},'%'))
        </if>
        order by crte_time
    </select>
    <select id="queryZYPage" resultType="cn.hsa.module.outpt.medictocare.dto.MedicToCareDTO">
        SELECT id as visitId,name ,cert_no as certNo ,in_dept_id as deptId,in_dept_name as deptName ,"2" as visitType
        from inpt_visit
