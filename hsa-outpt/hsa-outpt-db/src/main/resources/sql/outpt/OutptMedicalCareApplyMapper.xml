        order by crte_time
    </select>
    <select id="queryVisitById" resultType="cn.hsa.module.outpt.medictocare.dto.MedicToCareDTO">
        SELECT
            a.id as visitId,
            a.name,
            (case a.gender_code when  '1' then '男' when '2' then '女' else '' end) as  genderCode,
            a.age,
            a.cert_no as certNo,
            a.phone,
            '1' as changeType,
            a.dept_id as deptId,
            a.dept_name as deptName
        FROM outpt_visit a
        where a.hosp_code = #{hospCode}
          and a.id = #{visitId}

        union all

        SELECT
            a.id as visitId,
            a.name,
            (case a.gender_code when  '1' then '男' when '2' then '女' else '' end) as  genderCode,
            a.age,
            a.cert_no,
            a.phone,
            '2' as changeType,
            a.in_dept_id as deptId,
            a.in_dept_name as deptName
        FROM inpt_visit a
        where a.hosp_code = #{hospCode}
          and a.id = #{visitId}
    </select>
